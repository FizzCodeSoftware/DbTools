<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <PackageId>FizzCode.DbTools.SqlExecuter.MsSql</PackageId>
    <Authors>FizzCode Ltd.</Authors>
    <Product>FizzCode DbTools</Product>
    <Description>Microsoft SQL Server specific sql executer for Fizzcode DbTools project</Description>
    <Copyright>Copyright Â© 2019-2024 FizzCode Ltd.</Copyright>
    <RepositoryUrl>https://github.com/FizzcodeSoftware/DbTools</RepositoryUrl>
    <Platforms>AnyCPU</Platforms>
    <CodeAnalysisRuleSet>..\..\FizzCode.ruleset</CodeAnalysisRuleSet>
    <AssemblyName>FizzCode.DbTools.SqlExecuter.MsSql</AssemblyName>
    <RootNamespace>FizzCode.DbTools.SqlExecuter.MsSql</RootNamespace>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
	<NoWarn>CS1591</NoWarn>
    <ImplicitUsings>disable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Data.SqlClient" Version="5.1.5" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\DbTools.Common\DbTools.Common.csproj" />
    <ProjectReference Include="..\..\DbTools.Configuration\DbTools.Configuration.csproj" />
    <ProjectReference Include="..\..\DbTools.SqlExecuter\DbTools.SqlExecuter.csproj" />
  </ItemGroup>

</Project>
