<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <PackageId>FizzCode.DbTools.SqlGenerator.SqLite</PackageId>
    <Authors>FizzCode Ltd.</Authors>
    <Product>FizzCode DbTools</Product>
    <Description>SqLite specific database definition for Fizzcode DbTools project</Description>
    <Copyright>Copyright Â© 2019-2024 FizzCode Ltd.</Copyright>
    <RepositoryUrl>https://github.com/FizzcodeSoftware/DbTools</RepositoryUrl>
    <Platforms>AnyCPU</Platforms>
    <CodeAnalysisRuleSet>..\..\FizzCode.ruleset</CodeAnalysisRuleSet>
    <AssemblyName>FizzCode.DbTools.SqlGenerator.SqLite</AssemblyName>
    <RootNamespace>FizzCode.DbTools.SqlGenerator.SqLite</RootNamespace>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
	<ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>	
    <NoWarn>CS1591</NoWarn>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\DbTools.SqlGenerator.Base\DbTools.SqlGenerator.Base.csproj" />
  </ItemGroup>

</Project>
